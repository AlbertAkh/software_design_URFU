double minx=0.7;
        double maxx=1.2-minx;
        int viborka = 100;
        double sum=0;

        double x;
        double[] mas = new double[viborka];


        for (int i = 0; i<viborka;i++){
            x = Math.random()*maxx+0.7;
            mas[i] = x;
            sum+=x;
        }
        for (int i = 0; i<viborka;i++){
           System.out.print(mas[i]+" + ");
        }

        System.out.println(sum/100);
